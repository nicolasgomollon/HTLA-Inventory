<%= form_for [:admin, @computer] do |f| %>
	<%= label_tag(:template_id, "Computer Template:") %>
	<%= select_tag(:template_id, options_for_select(@template_select)) %>
	<%= f.label :serial, "Serial Number" %>
	<%= f.text_field :serial %>
	<%= f.label :idtag, "Tag" %>
	<%= f.text_field :idtag %>
	<%= f.submit %>
<% end %>