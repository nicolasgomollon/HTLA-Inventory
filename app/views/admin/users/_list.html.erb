<table class="table table-striped table-bordered table-condensed">
<thead>
	<tr>
		<th>Name</th>
		<th>Username</th>
		<th></th>
	</tr>
</thead>
<tbody>
<% users.each do |user| %>
	<tr>
		<td><%= user.fullname %></td>
		<td><%= user.username %></td>
		<td><%= link_to edit_admin_admin_user_path(user) do %>
				<button class="btn btn-primary">Change Password</button>
			<% end %>
		</td>
	</tr>
<% end %>
</tbody>
<caption>Ownerships</caption>
</table>