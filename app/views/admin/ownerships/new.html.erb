<script type="text/javascript">
	$(document).ready(function() {
		$("#datepicker").datepicker({dateFormat:"yy-mm-dd"})
	})
</script>
<%= form_for [:admin, @ownership] do |f| %>
	<%= f.label :student_id, "Student ID" %>
	<%= f.text_field :student_id %>
	<%= f.label :computer_id, "Computer ID" %>
	<%= f.text_field :computer_id %>
	<%= label_tag(:date, "Start Date") %>
	<%= text_field_tag(:date, Date.today.to_s, :id=>"datepicker") %>
	<%= f.submit %>
<% end %>