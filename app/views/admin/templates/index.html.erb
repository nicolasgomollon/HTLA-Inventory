<div class="row">
	<div class="span6">
			<table class="table table-striped table-bordered table-condensed">
				<thead>
					<tr>
						<th>Computer Model</th>
						<th>Parts</th>
					</tr>
				</thead>
				<tbody>
					<% @templates.each do |template| %>
					<tr>
						<td><%= link_to template.name, [:admin, template] %></td>
						<td><%= template.parts %></td>
					</tr>
					<% end %>
				</tbody>
			</table>
			<%= link_to new_admin_computer_template_path do %>
				<button class="btn btn-primary">Create Template</button>
			<% end %>
	</div>
</div>